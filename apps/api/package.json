{
	"name": "@my-app/api",
	"private": true,
	"version": "0.0.1",
	"scripts": {
		"build": "npm run schema:generate && rimraf dist && tsc && tsc-alias",
		"predev": "npm run schema:generate && npm run build",
		"dev": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"tsc-alias -w\" \"nodemon -q dist/index.js\"",
		"prestart": "npm run build",
		"start": "cross-env NODE_ENV=production node dist/index.js",
		"schema:generate": "npx prisma generate",
		"schema:format": "npx prisma format",
		"db:push": "npx prisma db push",
		"db:pull": "npx prisma db pull",
		"migration:reset": "npx prisma migrate reset",
		"migration:generate": "npx prisma migrate dev --create-only",
		"migration:migrate": "npx prisma migrate dev",
		"migration:deploy": "npx prisma migrate deploy",
		"test": "echo \"Error: no test specified\" && exit 1",
		"seed": "ts-node prisma/seed.ts"
	},
	"dependencies": {
		"@prisma/client": "5.18.0",
		"axios": "~1.6.0",
		"body-parser": "1.20.3",
		"cookie": "~0.7.0",
		"cors": "2.8.5",
		"cross-spawn": "~7.0.5",
		"dotenv": "16.4.5",
		"es-toolkit": "1.16.0",
		"express": "4.20.0",
		"express-list-endpoints": "7.1.0",
		"express-validator": "7.2.0",
		"helmet": "7.1.0",
		"lodash.pick": "~4.4.0",
		"morgan": "1.10.0",
		"nanoid": "~3.3.8",
		"path-to-regexp": "~0.1.10",
		"prisma": "5.18.0",
		"send": "~0.19.0",
		"serve-static": "~1.16.0",
		"slugify": "1.6.6",
		"ssh-keygen": "0.5.0",
		"sshpk": "1.18.0",
		"ts-node": "10.9.2",
		"underscore": "~1.12.1",
		"vite": "~4.4.12"
	},
	"devDependencies": {
		"@eslint/compat": "1.1.1",
		"@eslint/eslintrc": "3.1.0",
		"@eslint/js": "9.9.0",
		"@types/body-parser": "1.19.5",
		"@types/cors": "2.8.17",
		"@types/express": "4.17.21",
		"@types/morgan": "1.9.9",
		"@types/node": "22.5.1",
		"@types/sshpk": "^1.17.4",
		"concurrently": "8.2.2",
		"cross-env": "7.0.3",
		"eslint": "8.51.0",
		"eslint-import-resolver-alias": "1.1.2",
		"eslint-plugin-import": "2.28.1",
		"eslint-plugin-simple-import-sort": "10.0.0",
		"eslint-plugin-unused-imports": "3.0.0",
		"globals": "15.9.0",
		"nodemon": "3.1.4",
		"rimraf": "6.0.1",
		"tsc-alias": "1.8.10",
		"typescript": "5.5.4"
	}
}
